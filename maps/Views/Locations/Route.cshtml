@model maps.Models.LocationLists
@{
    ViewBag.Title = "Find Route";
}

<div class="row">
    <div class="col-md-12">
        <h3>Best Route</h3>
        <ol>
            @foreach (var item in Model.LocationList)
            {
                <li>@item.Code: @item.Address</li>
            }
        </ol>
    </div>
</div>

<div>
    @Html.ActionLink("Back", "Index", "Home")
</div>
